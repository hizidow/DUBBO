<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://dubbok.hz.netease.com/dubbo.xsd">
 
    <!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
    <dubbo:application name="dubbo-consumer-demo"  />
 
    <!-- 使用multicast广播注册中心暴露发现服务地址 -->
    <dubbo:registry address="zookeeper://10.170.164.121:2181" />
    
    <dubbo:consumer cluster="failfast" check="false"
		version="1.0" timeout="60000" filter="" />
 
		
	<!-- kaola-mobile工程接口 -->	
	<dubbo:reference group="performance" id="SearchPharseServiceFacade"
		interface="com.netease.haitao.mobile.facade.servitization.SearchPharseServiceFacade"
		url="dubbo://10.165.201.3:20880/com.netease.haitao.mobile.facade.servitization.SearchPharseServiceFacade"
		version="1.0" />
		
</beans>
